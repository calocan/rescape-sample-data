{
  "name": "rescape-sample-data",
  "version": "1.2.55",
  "description": "Sample data for rescape applications",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "install-peers": "cross-env node ./installPeersCommand.js | sh",
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "yarn run clean && yarn run lint && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-sample-data.git"
  },
  "author": "Andy Likuski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calocan/rescape-sample-data/issues"
  },
  "homepage": "https://github.com/calocan/resacape-sample-data",
  "dependencies": {
    "graphql": "^14.3.1",
    "graphql-geojson": "^1.0.0",
    "graphql-tools": "^4.0.4",
    "immutable": "^4.0.0-rc.9",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "parse-decimal-number": "^1.0.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "ramda-lens": "^0.1.2",
    "regenerator-runtime": "^0.13.2",
    "rescape-helpers": "^1.1.84",
    "rescape-ramda": "^1.5.41",
    "rescape-validate": "^1.2.21"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@types/jest": "^24.0.13",
    "autoprefixer": "9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "dotenv": "8.0.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "fs-extra": "8.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "jest-cli": "^24.8.0",
    "promise": "8.0.3",
    "react-dev-utils": "^9.0.1",
    "redux-devtools-extension": "^2.13.8",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-watch": "4.3.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "4.32.2",
    "webpack-dev-server": "3.4.1",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jestsetup.js",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": true,
              "regenerator": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
